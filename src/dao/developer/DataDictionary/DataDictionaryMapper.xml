<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="dao.developer.DataDictionary.DataDictionaryMapper">
 	<!-- 查询所有app的状态 -->
 	<select id="getstatusList" resultType="DataDictionary">
 		SELECT * FROM data_dictionary WHERE typeCode='APP_STATUS'
 	</select>
 	<!-- 查询所有的所属平台 -->
 	<select id="getflatformList" resultType="DataDictionary">
 		SELECT * FROM data_dictionary WHERE typeCode=#{typeCode}
 	</select>
 </mapper>